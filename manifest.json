{
    "manifest_version": 2,
    "name": "Launcher",
    "description": "Quick launch for social media",
    "version": "1.0.0",
    "author": "Melissa Jones @melissathedev1",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": ["activeTab"]
}